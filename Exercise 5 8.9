def show_messages(messages):
    """Prints each text message from the list"""
    for message in messages:
        print(message)

# Defining a list of short text messages
text_messages = [
    "Hello World!",
    "What are you up to?",
    "How are you doing?",
    "I'm just curious"
]

# Passing  the list to show_messages()
show_messages(text_messages)
